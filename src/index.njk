<section id="conference" class="row">
  <h1>The international Nodejs Conference in Italy
  </h1>
  <h4>
    30th September 2023 - Brescia, Italy</h4>
    <div class="row">
      <div class='conference-section'> 
        <div> {% renderFile "./src/contents/conference.md" %} </div>
        <div> 
          <section id="venue">
            {% renderFile "./src/contents/venue.md" %}
          </section>
          <section id="tickets">
            {% renderFile "./src/contents/tickets-disabled.md" %}
          </section>
        
      </div>
    </div>
  </section>

  <div id="map-venue"></div> 

  <section id="cfp" class='cfp'>
    {% renderFile "./src/contents/cfp.md" %}
    <a
      href="https://docs.google.com/forms/d/1qWxi6NRLyoroqqy3w7bDyGilO0QdN-xsTc6r4SHAx9Q/edit"
     >Submit your proposal</a>
  </section>

  {#
      <section id="to-be-defined">
        <div class="row">
          <div class="col">
            <h1>Agenda, Venue, Tickets</h1>
            <p>To be defined</p>
          </div>
        </div>
      </section>
      #}
  {#
      <section id="talks">
        <div class="row px-2">
        {%- for talk in collections.talks -%}
          <div class="col-4">
            <h3>{{ talk.data.title }}</h3>
            <div class="bio mb-2">
              {{ talk.data.bio | safe }}
            </div>
            <div class="description">
              {{ talk.templateContent | safe }}
            </div>
          </div>
        {%- endfor -%}
        </div>
      </section> 
      #}
  <section id="sponsor" class="mb-24">
    {% renderFile "./src/contents/sponsor.md" %}
    <div class="row">
      {%- for post in collections.sponsor_packages -%}
        <div class="col-12 col-md-6 col-lg-3 g-4">
          <div class="p-4 h-100 price-card">
            <h3 class="mb-0">{{ post.data.title }}</h3>
            <div class="price">
              <div class="fs-3">{{ post.data.price }} &euro;</div>
            </div>
            <hr>
              {{ post.templateContent | safe }} 
            </div>
          </div>
        {%- endfor -%}
      </div>
    </section>

    <section class="our-sponsor">
      {% renderFile "./src/includes/sponsors.md" %}
      <div class="gird-sponsor">

        {%- for sponsor in collections.sponsors -%}
          <a href="{{ sponsor.data.url }}" target="_blank">
            <div class="sponsor-image">
              <img src="{{ sponsor.data.image }}"> </img>
              <div class="badge">{{ sponsor.data.package }}</div>
            </div>
          </a>
        {%- endfor -%}
  
      </div>
      <a class="link-button" href="mailto:info@webdebs.org?subject=Sponsorship request">Become a sponsor</a>
    </section>

